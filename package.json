{
	"name": "orange-ui-kit",
	"version": "0.0.17",
	"type": "module",
	"main": "./dist/orange-ui-kit.js",
	"module": "./dist/orange-ui-kit.js",
	"scripts": {
		"dev": "vite",
		"build": "npm version patch --no-git-tag-version && vue-tsc -b && vite build",
		"publish": "npm run build && npm version patch -m \"chore(release): %s\" && npm publish",
		"preview": "vite preview",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"vue": ">=3"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^1.9.0",
		"@storybook/addon-essentials": "^8.3.3",
		"@storybook/addon-interactions": "^8.3.3",
		"@storybook/addon-links": "^8.3.3",
		"@storybook/addon-onboarding": "^8.3.3",
		"@storybook/blocks": "^8.3.3",
		"@storybook/test": "^8.3.3",
		"@storybook/vue3": "^8.3.3",
		"@storybook/vue3-vite": "^8.3.3",
		"@types/node": "^22.5.5",
		"@vitejs/plugin-vue": "^5.1.2",
		"sass-embedded": "^1.78.0",
		"storybook": "^8.3.3",
		"typescript": "^5.5.3",
		"vite": "^5.4.1",
		"vite-plugin-dts": "^4.2.1",
		"vue-tsc": "^2.0.29"
	},
	"peerDependencies": {
		"vue": ">=3"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/orange-ui-kit.js",
			"require": "./dist/orange-ui-kit.js",
			"types": "./dist/index.d.ts"
		},
		"./dist/style.css": {
			"import": "./dist/style.css",
			"require": "./dist/style.css"
		},
		"./styles": "./dist/style.css"
	},
	"sideEffects": [
		"*.css"
	]
}
